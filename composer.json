{
    "name": "swiss-devjoy/laravel-optimize-sqlite",
    "description": "Optimize your SQLite database for production in Laravel",
    "keywords": [
        "Swiss Devjoy",
        "laravel",
        "laravel-optimize-sqlite"
    ],
    "homepage": "https://github.com/swiss-devjoy/laravel-optimize-sqlite",
    "license": "MIT",
    "authors": [
        {
            "name": "Dimitri KÃ¶nig",
            "email": "dimitri@devjoy.ch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0||^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9||^3.0",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "SwissDevjoy\\LaravelOptimizeSqlite\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SwissDevjoy\\LaravelOptimizeSqlite\\LaravelOptimizeSqliteServiceProvider"
            ],
            "aliases": {
                "LaravelOptimizeSqlite": "SwissDevjoy\\LaravelOptimizeSqlite\\Facades\\LaravelOptimizeSqlite"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
